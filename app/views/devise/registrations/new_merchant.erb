  </div>
  <div class=" col-span-5">

    <div class="well">

      <%= form_for(resource, :as => resource_name, :url => register_path, :html => { :class => ''}) do |f| %>

        <input type="hidden" name="merc" value="1">

        <div class="control-group">
          <%= label :name, "Name", :class => "control-label" %><%= red_asterisk %>
          <div class="controls">
            <%= text_field_tag nil, nil, :name => "merchant[first_name]",  :placeholder => 'First',:autofocus => true   %>

            <%= error_on_form(resource.errors,:email)  %>
          </div>
        </div>

        <div class="control-group">
          <%= f.label :email, "Email", :class => "control-label" %> <%= red_asterisk %>
          <div class="controls">
            <%= f.email_field :email,  :placeholder => 'Email' %>

            <%= error_on_form(resource.errors,:email)  %>
          </div>
        </div>

        <div class="control-group">
          <%= f.label :password, "Password", :class => "control-label" %>  <%= red_asterisk %>
          <div class="controls">

            <%= f.password_field :password, :placeholder => 'Password' %>

            <%= error_on_form(resource.errors,:password)  %>
          </div>
        </div>

        <div class="control-group">
          <%= f.label :password_confirmation, "Confirm your password", :class => "control-label" %> <%= red_asterisk %>
          <div class="controls">
            <%= f.password_field :password_confirmation, :placeholder => 'Password' %>
            <%= error_on_form(resource.errors,:password_confirmation)  %>
          </div>
        </div>

        <div class="control-group">
          <div class="controls">
            <%= f.submit "Register", :class => "btn btn-primary" %>
          </div>
        </div>

      <% end %>
    </div>
  </div>
</div>