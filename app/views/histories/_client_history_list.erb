<% self.formats = [:json, :html] %>
<div class="client-history-container">
  <% carts.each do |c| %>
    <div class="history-header">
      <%= c.updated_at.to_date %>
    </div>
    <div class="history-products-container">
      <% c.items.each do |i| %>
          <%= render 'client_product', item: i %>
      <%end %>
    </div>
  <%end%>
</div>